{
  "private": true,
  "name": "tely",
  "description": "Live media sharing",
  "keywords": [
    "media",
    "discord",
    "share",
    "books",
    "movies",
    "songs"
  ],
  "version": "0.1.0",
  "author": "wyattades",
  "license": "UNLICENSE",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wyattades/tely.git"
  },
  "bugs": {
    "url": "https://github.com/wyattades/tely/issues"
  },
  "homepage": "https://github.com/wyattades/tely#readme",
  "main": "src/index.js",
  "scripts": {
    "lint": "eslint src",
    "dev": "NODE_ENV=development webpack-dev-server",
    "build": "NODE_ENV=production webpack",
    "deploy": "aws s3 rm s3://tely.app --recursive --exclude \"*\" --include \"*.js\" --include \"*.css\" && aws s3 sync dist s3://tely.app/ && cf-invalidate --wait -- $CFINVALIDATE index.html tely-sw.js",
    "build-deploy": "npm run build && npm run deploy"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free-webfonts": "^1.0.9",
    "bulma": "^0.7.4",
    "firebase": "^5.8.5",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "react-hot-loader": "^4.7.2",
    "react-router-dom": "^4.3.1",
    "react-url-query": "^1.4.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.3",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-config-react": "^1.1.7",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.18.3",
    "eslint-plugin-react-hooks": "^2.3.0",
    "prettier": "^1.19.1",
    "webpack-boiler": "^0.4.2"
  }
}
